{
  "name": "maid-service-platform",
  "version": "1.0.0",
  "description": "Full-stack maid service marketplace platform",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
